type Query {
    employees: [Employee]
}

scalar Date
scalar Object

type Employee {
    id: String!
    name: String!
    surname: String!
    birthDate: Date
    identityCode: String
    branchAssigns: [BranchAssign]
    customFields: [CustomField]
}

type CustomField {
    name: String
    value: Object
}

type BranchAssign {
    employeeId: String
    branchId: String
    startDate: Date
    endDate: Date
}



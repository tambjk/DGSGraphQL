type Query {
    employees: [Employee]
}
scalar Date

type Employee {
    id: String!
    name: String!
    surname: String!
    birthDate: Date
    identityCode: String
    branchAssigns: [BranchAssign]
}

type BranchAssign {
    employeeId: String
    branchId: String
    startDate: Date
    endDate: Date
}


